{
  "name": "mess-project-manager",
  "displayName": "Mess Project Manager",
  "description": "Advanced project manager with Git integration, terminal shortcuts, and Notes & Commands system. Organize multiple projects with categories, favorites, and drag-and-drop functionality.",
  "publisher": "DaoQuangMinh",
  "details":"sd",
  "repository": {
    "type": "git",
    "url": "https://github.com/MinhMuix2908/mess-project-manager.git"
  },
  "version": "0.3.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "project manager",
    "git integration",
    "terminal",
    "notes",
    "commands",
    "organization",
    "productivity",
    "workspace",
    "favorites",
    "categories"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Mess Project Manager",
      "properties": {
        "messProjectManager.showInactiveProjects": {
          "type": "boolean",
          "default": false,
          "description": "Show inactive projects in the tree view"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "messProjectManager",
          "title": "Mess Project Manager",
          "icon": "icons/logo/logo.svg"
        }
      ]
    },
    "views": {
      "messProjectManager": [
        {
          "id": "messProjectManagerTreeView",
          "name": "All Projects"
        },
        {
          "id": "messProjectManagerCategories",
          "name": "All Projects Category "
        },
        {
          "id": "messProjectManagerGit",
          "name": "Git "
        },
        {
          "id": "messProjectManagerNotes",
          "name": "Notes and Commands"
        }
      ]
    },
    "commands": [
      {
        "command": "messProjectManager.saveCurrentLocation",
        "title": "Save Current Location",
        "icon": "$(add)"
      },
      {
        "command": "messProjectManager.editProjectsConfig",
        "title": "Edit Projects Config",
        "icon": "$(edit)"
      },
      {
        "command": "messProjectManager.refreshProjects",
        "title": "Refresh Projects",
        "icon": "$(refresh)"
      },
      {
        "command": "messProjectManager.toggleShowInactive",
        "title": "Toggle Show Inactive Projects",
        "icon": "$(eye)"
      },
      {
        "command": "messProjectManager.openFileExplorerWindow",
        "title": "Open this in File Explorer",
        "icon": "$(folder-opened)"
      },
      {
        "command": "messProjectManager.openProjectNewWindow",
        "title": "Open in New Window",
        "icon": "$(empty-window)"
      },
      {
        "command": "messProjectManager.openProjectCurrentWindow",
        "title": "Open in Current Window",
        "icon": "$(arrow-right)"
      },
      {
        "command": "messProjectManager.addCategory",
        "title": "Add Category",
        "icon": "$(plus)"
      },
      {
        "command": "messProjectManager.assignCategory",
        "title": "Assign to Category",
        "icon": "$(tag)"
      },
      {
        "command": "messProjectManager.removeCategory",
        "title": "Remove Category",
        "icon": "$(trash)"
      },
      {
        "command": "messProjectManager.toggleFavorite",
        "title": "Toggle Favorite",
        "icon": "$(star)"
      },
      {
        "command": "messProjectManager.searchProjects",
        "title": "Search Projects",
        "icon": "$(search)"
      },
      {
        "command": "messProjectManager.clearSearch",
        "title": "Clear Search",
        "icon": "$(clear-all)"
      },
      {
        "command": "messProjectManager.refreshGitProjects",
        "title": "Refresh Git Projects",
        "icon": "$(refresh)"
      },
      {
        "command": "messProjectManager.cloneRepository",
        "title": "Clone Repository",
        "icon": "$(repo-clone)"
      },
      {
        "command": "messProjectManager.pullRepository",
        "title": "Git Pull",
        "icon": "$(arrow-down)"
      },
      {
        "command": "messProjectManager.showGitStatus",
        "title": "Show Git Status",
        "icon": "$(git-branch)"
      },
      {
        "command": "messProjectManager.openTerminalMenu",
        "title": "Open Terminal Menu",
        "icon": "$(terminal)"
      },
      {
        "command": "messProjectManager.openIntegratedTerminal",
        "title": "Open Integrated Terminal",
        "icon": "$(terminal-powershell)"
      },
      {
        "command": "messProjectManager.openCommandPrompt",
        "title": "Open Command Prompt",
        "icon": "$(terminal-cmd)"
      },
      {
        "command": "messProjectManager.openCommandPromptAdmin",
        "title": "Open Command Prompt (Admin)",
        "icon": "$(shield)"
      },
      {
        "command": "messProjectManager.openPowerShell",
        "title": "Open PowerShell",
        "icon": "$(terminal-powershell)"
      },
      {
        "command": "messProjectManager.openPowerShellAdmin",
        "title": "Open PowerShell (Admin)",
        "icon": "$(shield)"
      },
      {
        "command": "messProjectManager.openGitBash",
        "title": "Open Git Bash",
        "icon": "$(terminal-bash)"
      },
      {
        "command": "messProjectManager.refreshNotes",
        "title": "Refresh Notes",
        "icon": "$(refresh)"
      },
      {
        "command": "messProjectManager.copyNoteContent",
        "title": "Copy Content",
        "icon": "$(copy)"
      },
      {
        "command": "messProjectManager.runCommand",
        "title": "Run Command",
        "icon": "$(play)"
      },
      {
        "command": "messProjectManager.addSheet",
        "title": "Add Sheet",
        "icon": "$(new-folder)"
      },
      {
        "command": "messProjectManager.openSheet",
        "title": "Open Sheet",
        "icon": "$(go-to-file)"
      },
      {
        "command": "messProjectManager.deleteSheet",
        "title": "Delete Sheet",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "messProjectManager.saveCurrentLocation",
          "when": "view == messProjectManagerTreeView",
          "group": "navigation@1"
        },
        {
          "command": "messProjectManager.editProjectsConfig",
          "when": "view == messProjectManagerTreeView",
          "group": "navigation@2"
        },
        {
          "command": "messProjectManager.refreshProjects",
          "when": "view == messProjectManagerTreeView",
          "group": "navigation@3"
        },
        {
          "command": "messProjectManager.searchProjects",
          "when": "view == messProjectManagerTreeView",
          "group": "navigation@4"
        },
        {
          "command": "messProjectManager.clearSearch",
          "when": "view == messProjectManagerTreeView",
          "group": "navigation@5"
        },
        {
          "command": "messProjectManager.toggleShowInactive",
          "when": "(view == messProjectManagerTreeView) && !messProjectManager.showInactiveProjects",
          "group": "navigation@6"
        },
        {
          "command": "messProjectManager.addCategory",
          "when": "view == messProjectManagerCategories",
          "group": "navigation@7"
        },
        {
          "command": "messProjectManager.removeCategory",
          "when": "view == messProjectManagerCategories",
          "group": "navigation@8"
        },
        {
          "command": "messProjectManager.refreshGitProjects",
          "when": "view == messProjectManagerGit",
          "group": "navigation@1"
        },
        {
          "command": "messProjectManager.cloneRepository",
          "when": "view == messProjectManagerGit",
          "group": "navigation@2"
        },
        {
          "command": "messProjectManager.addSheet",
          "when": "view == messProjectManagerNotes",
          "group": "navigation@1"
        },
        {
          "command": "messProjectManager.refreshNotes",
          "when": "view == messProjectManagerNotes",
          "group": "navigation@2"
        }
      ],
      "view/item/context": [
        {
          "command": "messProjectManager.openFileExplorerWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "inline@1"
        },
        {
          "command": "messProjectManager.openProjectNewWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "inline@2"
        },
        {
          "command": "messProjectManager.openProjectCurrentWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "inline@3"
        },
        {
          "command": "messProjectManager.openIntegratedTerminal",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "inline@4"
        },
        {
          "command": "messProjectManager.openFileExplorerWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "1_open@1"
        },
        {
          "command": "messProjectManager.openProjectNewWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "1_open@2"
        },
        {
          "command": "messProjectManager.openProjectCurrentWindow",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "1_open@3"
        },
        {
          "command": "messProjectManager.toggleFavorite",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories) && viewItem == projectItem",
          "group": "2_organize@1"
        },
        {
          "command": "messProjectManager.assignCategory",
          "when": "view == messProjectManagerCategories && viewItem == projectItem",
          "group": "2_organize@2"
        },
        {
          "command": "messProjectManager.pullRepository",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "inline@1"
        },
        {
          "command": "messProjectManager.showGitStatus",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "inline@2"
        },
        {
          "command": "messProjectManager.openFileExplorerWindow",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "inline@3"
        },
        {
          "command": "messProjectManager.openIntegratedTerminal",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "inline@4"
        },
        {
          "command": "messProjectManager.pullRepository",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "3_git@1"
        },
        {
          "command": "messProjectManager.showGitStatus",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "3_git@2"
        },
        {
          "command": "messProjectManager.openProjectNewWindow",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "1_open@1"
        },
        {
          "command": "messProjectManager.openProjectCurrentWindow",
          "when": "view == messProjectManagerGit && viewItem == projectItem",
          "group": "1_open@2"
        },
        {
          "command": "messProjectManager.openTerminalMenu",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories || view == messProjectManagerGit) && viewItem == projectItem",
          "group": "4_terminal@1"
        },
        {
          "command": "messProjectManager.openIntegratedTerminal",
          "when": "(view == messProjectManagerTreeView || view == messProjectManagerCategories || view == messProjectManagerGit) && viewItem == projectItem",
          "group": "4_terminal@2"
        },
        {
          "command": "messProjectManager.copyNoteContent",
          "when": "view == messProjectManagerNotes && (viewItem == noteItem || viewItem == commandItem)",
          "group": "inline@1"
        },
        {
          "command": "messProjectManager.runCommand",
          "when": "view == messProjectManagerNotes && viewItem == commandItem",
          "group": "inline@2"
        },
        {
          "command": "messProjectManager.copyNoteContent",
          "when": "view == messProjectManagerNotes && (viewItem == noteItem || viewItem == commandItem)",
          "group": "1_actions@1"
        },
        {
          "command": "messProjectManager.runCommand",
          "when": "view == messProjectManagerNotes && viewItem == commandItem",
          "group": "1_actions@2"
        },
        {
          "command": "messProjectManager.openSheet",
          "when": "view == messProjectManagerNotes && viewItem == sheetItem",
          "group": "inline@1"
        },
        {
          "command": "messProjectManager.deleteSheet",
          "when": "view == messProjectManagerNotes && viewItem == sheetItem",
          "group": "inline@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.90.0",
    "tslint": "^6.1.3",
    "typescript": "^5.0.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@types/react": "^19.1.12"
  }
}
